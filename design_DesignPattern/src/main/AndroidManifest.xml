<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.sxkeji.shixindesignpattern">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_ez"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="net.designpattern.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".strategy2.StrategyActivity"></activity>
        <activity android:name="net.designpattern.proxy.statics.StaticProxyActivity"></activity>
        <activity android:name="net.designpattern.proxy.dyn.DynProxyActivity"></activity>
        <activity android:name="net.designpattern.chain_of_responsibility.ChainofResponsibilityActivity"></activity>
        <activity android:name="net.designpattern.adapter.AdapterActivity"></activity>
    </application>

</manifest>